<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.wondersgroup.api.dao.TestMapper">
    <!-- parameterType:参数类型,填写实体类的完整名字 -->

    <select id="findById" parameterType="String" resultType="com.wondersgroup.api.entity.User">
        select * from tb_user1 where uid=#{id}
    </select>
    <select id="findAll"  resultType="com.wondersgroup.api.entity.User">
        select * from tb_user1 where 1=1
    </select>

</mapper>
